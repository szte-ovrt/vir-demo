<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.PasswordField?>
<VBox alignment="CENTER" prefHeight="407.0" prefWidth="530.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.sed.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Label text="Username" />
      <TextField fx:id="usernameTF" accessibleHelp="Provide your username" accessibleText="Provide your username" />
      <Label text="Password" />
      <PasswordField fx:id="passwordTF" accessibleHelp="Provide your password" accessibleText="Provide your password" />
      <Button mnemonicParsing="false" onAction="#login" text="Login" />
   </children>
</VBox>
